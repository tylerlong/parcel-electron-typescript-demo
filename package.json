  "scripts": {
    "serve": "yarn parcel build && electron .",
    "reset": "rm -rf .parcel-cache && rm -rf dist && rm -rf node_modules && rm yarn.lock && yarn install"
  },
  "targets": {
    "electron": {
      "source": "src/electron.ts",
      "context": "electron-main"
    },
    "renderer": {
      "source": "src/index.html",
      "context": "electron-renderer",
      "publicUrl": "./"
    }
  },
  "main": "dist/electron/electron.js",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "gts": "^3.1.1",
    "parcel": "^2.8.3",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  },
  "dependencies": {
    "electron": "^23.0.0"
  }
}
