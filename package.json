{
  "name": "parcel-electron-typescript-demo",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "serve": "yarn parcel --target renderer --no-cache",
    "build": "rm -rf build && yarn parcel build --no-cache --dist-dir=build",
    "start": "yarn build && electron .",
    "release": "ts-node -r dotenv-override-true/config src/electron-builder.ts"
  },
  "targets": {
    "electron": {
      "source": "src/electron.ts",
      "context": "electron-main"
    },
    "preload": {
      "source": "src/preload.ts",
      "context": "node",
      "distDir": "build/renderer"
    },
    "renderer": {
      "source": "src/index.html",
      "context": "browser",
      "publicUrl": "."
    }
  },
  "main": "build/electron/electron.js",
  "devDependencies": {
    "@tylerlong/use-proxy": "^1.1.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "antd-mobile": "^5.28.0",
    "dotenv-override-true": "^6.2.2",
    "electron": "^23.0.0",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.2",
    "events": "^3.3.0",
    "gts": "^3.1.1",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yarn-upgrade-all": "^0.7.2"
  }
}
